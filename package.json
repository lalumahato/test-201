  "bugs": {
    "url": "https://github.com/lalumahato/test-201/issues"
  },
  "homepage": "https://github.com/lalumahato/test-201#readme",
  "dependencies": {
    "cookie-parser": "1.4.3",
    "cors": "2.8.5",
    "debug": "2.6.9",
    "dotenv": "10.0.0",
    "ejs": "3.1.6",
    "express": "4.17.1",
    "helmet": "3.12.0",
    "module-alias": "2.2.2",
    "sequelize": "^6.12.4",
    "sequelize-cli": "6.3.0",
    "swagger-ui-express": "4.3.0",
    "winston": "3.3.3",
    "winston-daily-rotate-file": "4.5.5"
